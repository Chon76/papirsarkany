{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "testing-library"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals"
  ],
  "ignorePatterns": ["**/*.js", "**/*.cjs"],
  "root": true,
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
